{
  "name": "@web2labs/studio-skill",
  "version": "1.0.1",
  "description": "OpenClaw skill for Web2Labs Studio - AI-powered video editing",
  "type": "module",
  "main": "src/server.mjs",
  "scripts": {
    "start": "node src/server.mjs",
    "test": "node --test test/*.test.mjs",
    "test:integration": "node --test ../tests/integration/*.test.mjs",
    "test:e2e": "node --test ../tests/e2e/*.test.mjs",
    "test:live": "node --test ../tests/live/*.test.mjs",
    "test:hardening": "npm test && npm run test:integration && npm run test:e2e",
    "lint": "node --check src/server.mjs src/lib/*.mjs src/tools/*.mjs",
    "sync": "../scripts/sync-skill-repo.sh",
    "publish:clawhub": "../scripts/publish-to-clawhub.sh"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "form-data": "^4.0.5",
    "node-fetch": "^3.3.2",
    "socket.io-client": "^4.8.3",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "openclaw",
    "video-editing",
    "ai",
    "youtube",
    "shorts"
  ],
  "author": "Web2Labs <hello@web2labs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/web2labs/web2labs-studio-skill"
  }
}
